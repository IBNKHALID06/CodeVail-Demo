{
  "name": "codevail-interview-platform",
  "version": "0.1.0",
  "private": true,
  "description": "CodeVail Interview Platform desktop app",
  "author": "CodeVail",
  "main": "electron/main.js",
  "scripts": {
    "build": "next build",
    "export": "next export",
    "dev": "next dev",
    "dev:signal": "node server/signaling-server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:signal\"",
    "dev:backend": "cd backend && python app.py",
    "dev:full": "python start_dev.py",
    "init:db": "python init_database.py",
    "lint": "next lint",
    "start": "next start",
    "start:backend": "cd backend && python app.py",
    "start:full": "python start_production.py",
    "start:prod": "python start_production.py",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"electron .\"",
    "electron:pack": "electron-builder",
    "electron:dist": "pnpm run build && electron-builder",
    "electron:dist:skip-build": "electron-builder",
    "preelectron:pack": "pnpm run build",
    "lint:css": "stylelint \"**/*.css\" --fix"
  },
  "dependencies": {
    "@expo/vector-icons": "latest",
    "@hookform/resolvers": "^3.9.1",
    "@monaco-editor/react": "latest",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "@react-navigation/drawer": "latest",
    "@shadcn/ui": "^0.0.4",
    "@ts-morph/common": "^0.28.0",
    "@vercel/analytics": "1.3.1",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.5.1",
    "expo": "latest",
    "expo-clipboard": "latest",
    "expo-constants": "latest",
    "expo-font": "latest",
    "expo-linear-gradient": "latest",
    "expo-linking": "latest",
    "expo-router": "latest",
    "geist": "^1.3.1",
    "input-otp": "1.4.1",
    "lucide-react": "^0.454.0",
    "monaco-editor": "latest",
    "next": "14.2.16",
    "next-themes": "^0.4.4",
    "react": "^18",
    "react-day-picker": "9.8.0",
    "react-dom": "^18",
    "react-hook-form": "^7.54.1",
    "react-native": "latest",
    "react-native-reanimated": "latest",
    "react-native-safe-area-context": "latest",
    "react-native-screens": "latest",
    "react-resizable-panels": "^2.1.7",
    "react-router-dom": "latest",
    "recharts": "2.15.0",
    "shadcn": "latest",
    "sonner": "^1.7.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "ts-morph": "^27.0.0",
    "vaul": "^0.9.6",
    "ws": "^8.18.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@next/swc-win32-x64-msvc": "^14.2.16",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^9.2.1",
    "electron": "38",
    "electron-builder": "^26.0.12",
    "postcss": "^8.5",
    "stylelint": "^16.8.0",
    "stylelint-config-standard": "^36.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "wait-on": "^8.0.4",
    "worker-loader": "^3.0.8"
  },
  "build": {
    "appId": "com.codevail.interview-platform",
    "productName": "CodeVail Interview Platform",
    "asar": false,
    "npmRebuild": false,
    "nodeGypRebuild": false,
    "buildDependenciesFromSource": false,
    "extraMetadata": {
      "dependencies": {}
    },
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron/**/*",
      "out/**/*"
    ],
    "asarUnpack": [],
    "artifactName": "CodeVail-${version}-${os}-${arch}.${ext}",
    "mac": {
      "category": "public.app-category.productivity",
      "target": "dmg"
    },
    "win": {
      "forceCodeSigning": false,
      "signAndEditExecutable": false,
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "allowToChangeInstallationDirectory": false
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
